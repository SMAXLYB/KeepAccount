    <!--    android:id="@+id/moreSettingActivity"-->
    <!--    android:name="life.chenshi.keepaccounts.module.setting.ui.moresetting.MoreSettingActivity"-->
    <!--    android:label="MoreSettingActivity"-->
    <!--    tools:layout="@layout/setting_activity_more_setting" />-->
</navigation>